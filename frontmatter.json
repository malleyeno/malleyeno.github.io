{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "_art-assets",
      "path": "[[workspace]]/_art-assets"
    },
    {
      "title": "images",
      "path": "[[workspace]]/assets/images"
    }
  ],
  "frontMatter.framework.startCommand": "jekyll serve --livereload",
  "frontMatter.preview.host": "http://127.0.0.1:4000",
  "frontMatter.website.host": "malleyeno.com"
}